<?xml version="1.0" ?>

<robot name="delivery_robot">
  <gazebo reference="camera_link">
    <material>Gazebo/Grey</material>
  </gazebo>
  <gazebo reference="left_wheel">
    <material>Gazebo/Indigo</material>
  </gazebo>
  <gazebo reference="right_wheel">
    <material>Gazebo/Indigo</material>
  </gazebo>
  <gazebo reference="base_chassis">
    <material>Gazebo/DarkGrey</material>
  </gazebo>
  <gazebo reference="front_left_caster">
    <material>Gazebo/White</material>
  </gazebo>
  <gazebo reference="front_right_caster">
    <material>Gazebo/White</material>
  </gazebo>
  <gazebo reference="back_left_caster">
    <material>Gazebo/White</material>
  </gazebo>
  <gazebo reference="back_right_caster">
    <material>Gazebo/White</material>
  </gazebo>
  <!-- RGBD Camera -->
  <gazebo reference="camera_link">
    <sensor name="camera1" type="depth">
      <always_on>1</always_on>
      <update_rate>20.0</update_rate>
      <visualize>true</visualize>
      <camera>
        <horizontal_fov>1.047</horizontal_fov>
        <image>
          <width>640</width>
          <height>480</height>
          <format>R8G8B8</format>
        </image>
        <depth_camera>

        </depth_camera>
        <clip>
          <near>0.1</near>
          <far>20</far>
        </clip>
      </camera>
      <plugin filename="libgazebo_ros_openni_kinect.so" name="camera_controller">
        <alwaysOn>true</alwaysOn>
        <updateRate>10.0</updateRate>
        <cameraName>camera</cameraName>
        <frameName>camera_link_optical</frameName>
        <imageTopicName>rgb/image_raw</imageTopicName>
        <depthImageTopicName>depth/image_raw</depthImageTopicName>
        <pointCloudTopicName>depth/points</pointCloudTopicName>
        <cameraInfoTopicName>rgb/camera_info</cameraInfoTopicName>
        <depthImageCameraInfoTopicName>depth/camera_info</depthImageCameraInfoTopicName>
        <pointCloudCutoff>0.4</pointCloudCutoff>
        <hackBaseline>0.07</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
        <CxPrime>0.0</CxPrime>
        <Cx>0.0</Cx>
        <Cy>0.0</Cy>
        <focalLength>0.0</focalLength>
      </plugin>
    </sensor>
  </gazebo>
  <!-- hokuyo -->
  <gazebo reference="hokuyo">
    <sensor name="head_hokuyo_sensor" type="ray">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>40</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>720</samples>
            <resolution>1</resolution>
            <min_angle>-1.570796</min_angle>
            <max_angle>1.570796</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.10</min>
          <max>30.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <!-- Noise parameters based on published spec for Hokuyo laser
               achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
               stddev of 0.01m will put 99.7% of samples within 0.03m of the true
               reading. -->
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin filename="libgazebo_ros_laser.so" name="gazebo_ros_head_hokuyo_controller">
        <topicName>/scan</topicName>
        <frameName>hokuyo</frameName>
      </plugin>
    </sensor>
  </gazebo>
  <!-- Differential drive. Set param "wheelSeparation" accordingly!-->
  <gazebo>
    <plugin filename="libgazebo_ros_diff_drive.so" name="differential_drive_controller">
      <legacyMode>false</legacyMode>
      <alwaysOn>true</alwaysOn>
      <updateRate>10</updateRate>
      <leftJoint>left_wheel_hinge</leftJoint>
      <rightJoint>right_wheel_hinge</rightJoint>
      <wheelSeparation>0.43</wheelSeparation>
      <wheelDiameter>0.16</wheelDiameter>
      <torque>20</torque>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <robotBaseFrame>robot_footprint</robotBaseFrame>
      <publishWheelTF>false</publishWheelTF>
      <publishWheelJointState>false</publishWheelJointState>
      <rosDebugLevel>na</rosDebugLevel>
      <wheelAcceleration>1.0</wheelAcceleration>
      <wheelTorque>20</wheelTorque>
      <odometrySource>world</odometrySource>
      <publishTf>1</publishTf>
      <publishOdomTF>true</publishOdomTF>
    </plugin>
  </gazebo>
  <!-- Robot definitions -->
  <link name="robot_footprint"/>
  <joint name="robot_footprint_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="robot_footprint"/>
    <child link="base_chassis"/>
  </joint>
  <link name="base_chassis">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="15"/>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.22812500000000002" ixy="0" ixz="0" iyy="0.340625" iyz="0" izz="0.5125000000000001"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.4 0.15"/>
      </geometry>
    </collision>
    <visual name="base_chassis_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.4 0.15"/>
      </geometry>
    </visual>
  </link>
  <!-- Wheels definitions -->
  <link name="left_wheel">
    <inertial>
      <mass value="1.0"/>
      <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0"/>
      <inertia ixx="0.0016333333333333332" ixy="0" ixz="0" iyy="0.0016333333333333332" iyz="0" izz="0.0032"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.08"/>
      </geometry>
    </collision>
    <visual name="left_wheel_visual">
      <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.08"/>
      </geometry>
    </visual>
  </link>
  <joint name="left_wheel_hinge" type="continuous">
    <origin rpy="0 0 0" xyz="0 0.21500000000000002 -0.0375"/>
    <child link="left_wheel"/>
    <parent link="base_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <dynamics damping="0" friction="1.0"/>
  </joint>
  <link name="right_wheel">
    <inertial>
      <mass value="1.0"/>
      <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0"/>
      <inertia ixx="0.0016333333333333332" ixy="0" ixz="0" iyy="0.0016333333333333332" iyz="0" izz="0.0032"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.08"/>
      </geometry>
    </collision>
    <visual name="right_wheel_visual">
      <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.08"/>
      </geometry>
    </visual>
  </link>
  <joint name="right_wheel_hinge" type="continuous">
    <origin rpy="0 0 0" xyz="0 -0.21500000000000002 -0.0375"/>
    <child link="right_wheel"/>
    <parent link="base_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <dynamics damping="0" friction="1.0"/>
  </joint>
  <link name="front_left_caster">
    <inertial>
      <mass value="0.1"/>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <inertia ixx="1.8062500000000002e-05" ixy="0" ixz="0" iyy="1.8062500000000002e-05" iyz="0" izz="1.8062500000000002e-05"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.02125"/>
      </geometry>
    </collision>
    <visual name="front_left_caster_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.02125"/>
      </geometry>
    </visual>
  </link>
  <joint name="front_left_caster_hinge" type="continuous">
    <origin rpy="0 0 0" xyz="0.234375 0.184375 -0.09625"/>
    <child link="front_left_caster"/>
    <parent link="base_chassis"/>
    <axis rpy="0 0 0" xyz="1 1 1"/>
    <dynamics damping="0" friction="0"/>
  </joint>
  <link name="front_right_caster">
    <inertial>
      <mass value="0.1"/>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <inertia ixx="1.8062500000000002e-05" ixy="0" ixz="0" iyy="1.8062500000000002e-05" iyz="0" izz="1.8062500000000002e-05"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.02125"/>
      </geometry>
    </collision>
    <visual name="front_right_caster_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.02125"/>
      </geometry>
    </visual>
  </link>
  <joint name="front_right_caster_hinge" type="continuous">
    <origin rpy="0 0 0" xyz="0.234375 -0.184375 -0.09625"/>
    <child link="front_right_caster"/>
    <parent link="base_chassis"/>
    <axis rpy="0 0 0" xyz="1 1 1"/>
    <dynamics damping="0" friction="0"/>
  </joint>
  <link name="back_left_caster">
    <inertial>
      <mass value="0.1"/>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <inertia ixx="1.8062500000000002e-05" ixy="0" ixz="0" iyy="1.8062500000000002e-05" iyz="0" izz="1.8062500000000002e-05"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.02125"/>
      </geometry>
    </collision>
    <visual name="back_left_caster_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.02125"/>
      </geometry>
    </visual>
  </link>
  <joint name="back_left_caster_hinge" type="continuous">
    <origin rpy="0 0 0" xyz="-0.234375 0.184375 -0.09625"/>
    <child link="back_left_caster"/>
    <parent link="base_chassis"/>
    <axis rpy="0 0 0" xyz="1 1 1"/>
    <dynamics damping="0" friction="0"/>
  </joint>
  <link name="back_right_caster">
    <inertial>
      <mass value="0.1"/>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <inertia ixx="1.8062500000000002e-05" ixy="0" ixz="0" iyy="1.8062500000000002e-05" iyz="0" izz="1.8062500000000002e-05"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.02125"/>
      </geometry>
    </collision>
    <visual name="back_right_caster_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.02125"/>
      </geometry>
    </visual>
  </link>
  <joint name="back_right_caster_hinge" type="continuous">
    <origin rpy="0 0 0" xyz="-0.234375 -0.184375 -0.09625"/>
    <child link="back_right_caster"/>
    <parent link="base_chassis"/>
    <axis rpy="0 0 0" xyz="1 1 1"/>
    <dynamics damping="0" friction="0"/>
  </joint>
  <link name="left_container_side">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="0.01"/>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <inertia ixx="8.416666666666669e-06" ixy="0" ixz="0" iyy="0.00021666666666666668" iyz="0" izz="0.00020841666666666665"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.01 0.1"/>
      </geometry>
    </collision>
    <visual name="base_chassis_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.01 0.1"/>
      </geometry>
    </visual>
  </link>
  <joint name="left_container_side_hinge" type="fixed">
    <origin rpy="0 0 0" xyz="0.0 0.195 0.125"/>
    <child link="left_container_side"/>
    <parent link="base_chassis"/>
  </joint>
  <link name="right_container_side">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="0.01"/>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <inertia ixx="8.416666666666669e-06" ixy="0" ixz="0" iyy="0.00021666666666666668" iyz="0" izz="0.00020841666666666665"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.01 0.1"/>
      </geometry>
    </collision>
    <visual name="base_chassis_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.01 0.1"/>
      </geometry>
    </visual>
  </link>
  <joint name="right_container_side_hinge" type="fixed">
    <origin rpy="0 0 0" xyz="0.0 -0.195 0.125"/>
    <child link="right_container_side"/>
    <parent link="base_chassis"/>
  </joint>
  <link name="front_container_side">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="0.01"/>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0001416666666666667" ixy="0" ixz="0" iyy="1.666666666666667e-05" iyz="0" izz="0.0001416666666666667"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.4 0.1"/>
      </geometry>
    </collision>
    <visual name="base_chassis_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.4 0.1"/>
      </geometry>
    </visual>
  </link>
  <joint name="front_container_side_hinge" type="fixed">
    <origin rpy="0 0 0" xyz="0.2 0.0 0.125"/>
    <child link="front_container_side"/>
    <parent link="base_chassis"/>
  </joint>
  <link name="back_container_side">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="0.01"/>
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0001416666666666667" ixy="0" ixz="0" iyy="8.416666666666669e-06" iyz="0" izz="0.0001334166666666667"/>
    </inertial>
    <collision name="collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.4 0.1"/>
      </geometry>
    </collision>
    <visual name="base_chassis_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.4 0.1"/>
      </geometry>
    </visual>
  </link>
  <joint name="back_container_side_hinge" type="fixed">
    <origin rpy="0 0 0" xyz="-0.245 0.0 0.125"/>
    <child link="back_container_side"/>
    <parent link="base_chassis"/>
  </joint>
  <!-- ### CAMERA ### -->
  <link name="camera_link">
    <inertial>
      <mass value="0.1"/>
      <origin rpy=" 0 0 0" xyz="0.0 0 0"/>
      <inertia ixx="4.166666666666668e-05" ixy="0" ixz="0" iyy="4.166666666666668e-05" iyz="0" izz="4.166666666666668e-05"/>
    </inertial>
    <collision name="camera_collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>
    <visual name="camera_link_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </visual>
  </link>
  <link name="camera_link_optical">
  </link>
  <joint name="camera_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.275 0 0.049999999999999996"/>
    <child link="camera_link"/>
    <parent link="base_chassis"/>
    <axis rpy="0 0 0.02" xyz="0 1 0"/>
  </joint>
  <joint name="camera_optical_joint" type="fixed">
    <origin rpy="-1.5707 0 -1.5707" xyz="0 0 0"/>
    <parent link="camera_link"/>
    <child link="camera_link_optical"/>
  </joint>
  <!-- ### LIDAR ### -->
  <link name="hokuyo">
    <inertial>
      <mass value="0.00001"/>
      <origin rpy=" 0 0 0" xyz="0.0 0 0"/>
      <inertia ixx="1.666666666666667e-08" ixy="0" ixz="0" iyy="1.666666666666667e-08" iyz="0" izz="1.666666666666667e-08"/>
    </inertial>
    <collision name="hokuyo_collision">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
    <visual name="hokuyo_visual">
      <origin rpy=" 0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="/models/meshes/hokuyo.dae"/>
      </geometry>
    </visual>
  </link>
  <joint name="hokuyo_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.2 0 0.225"/>
    <child link="hokuyo"/>
    <parent link="base_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
  </joint>
</robot>
